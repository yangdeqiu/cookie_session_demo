<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://www.ibatis.com/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.ydq.mapper.UserMapper">
    <select id="getUser" parameterType="User" resultType="User">
         select * from user where name=#{name} and password=#{password};
    </select>
</mapper>